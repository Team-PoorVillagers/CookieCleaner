{
	"manifest_version" : 2,

	"name" : "Cookie Cleaner" ,

	"description" : "remove cookies for a particular website" ,

	"version" : "1.0",

	"browser_action" : {
		"default_icon": "icon.png"
	},

	"permissions": ["cookies","https://*/","http://*/" , "activeTab" , "tabs"],

	"background": {
    	"scripts": ["background.js"]
  	}
}
